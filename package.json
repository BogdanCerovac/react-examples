{
  "name": "react-examples",
  "version": "1.0.0",
  "description": "Examples for [React](http://reactjs.com) and [ReactJS.NET](http://reactjs.net/).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/react-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/react-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/react-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate",
      "pre-push": "npm run validate"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx --cache",
    "eslint-fix": "eslint . --ext .js,.jsx --cache --fix",
    "openserver": "node \"./Util 02 - HTTP and REST servers/server.js\" 8040",
    "openserver_old": "node \"./Util 01 - Static File Server/static-file-server.js\" 8080",
    "prettier": "prettier --list-different '**/*.{json,js,jsx,ts,tsx}'",
    "prettier-fix": "prettier --write '**/*.{json,js,jsx,ts,tsx}'",
    "validate": "npm run eslint && npm run prettier"
  },
  "devDependencies": {
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "@types/systemjs": "^0.20.6",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^0.0.0",
    "husky": "^1.2.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/standalone": "^7.1.0",
    "babel-polyfill": "^6.26.0",
    "create-react-class": "15.6.3",
    "htmltojsx": "^0.3.0",
    "http-server": "0.11.1",
    "mime-types": "^2.1.21",
    "plugin-typescript": "8.0.0",
    "prop-types": "^15.6.2",
    "react": "^0.0.0-ed4c4a51c",
    "react-dom": "^0.0.0-ed4c4a51c",
    "react-spring": "^6.1.9",
    "react-transition-group": "^2.5.0",
    "schedule": "^0.5.0",
    "styled-components": "^4.1.1",
    "systemjs": "0.21.5",
    "systemjs-plugin-babel": "^0.0.25",
    "typescript": "^3.1.6"
  }
}
