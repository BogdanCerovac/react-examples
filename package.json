{
  "name": "react-examples",
  "version": "1.0.0",
  "description": "Examples for [React](http://reactjs.com) and [ReactJS.NET](http://reactjs.net/).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/react-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/react-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/react-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate",
      "pre-push": "npm run validate"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx --cache",
    "eslint-fix": "eslint . --ext .js,.jsx --cache --fix",
    "prettier": "prettier --list-different '**/*.{json,js,jsx,html,ts,tsx}'",
    "prettier-fix": "prettier --write '**/*.{json,js,jsx,html,ts,tsx}'",
    "start": "node \"./Util - 01 - Custom servers/server.js\" 8040",
    "validate": "npm run eslint && npm run prettier"
  },
  "devDependencies": {
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/systemjs": "^0.20.6",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.4.0",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/standalone": "^7.4.5",
    "create-react-class": "15.6.3",
    "htmltojsx": "^0.3.0",
    "mime-types": "^2.1.24",
    "plugin-typescript": "8.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-cache": "^2.0.0-alpha.1",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.1",
    "react-spring": "^8.0.21",
    "react-transition-group": "^4.1.0",
    "redux": "^4.0.1",
    "scheduler": "0.13.0",
    "serve": "^11.0.1",
    "styled-components": "^4.2.1",
    "systemjs": "^0.21.6",
    "systemjs-plugin-babel": "^0.0.25",
    "typescript": "^3.5.1"
  }
}
