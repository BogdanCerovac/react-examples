{
  "name": "react-examples",
  "version": "1.0.0",
  "description": "Examples for [React](http://reactjs.com) and [ReactJS.NET](http://reactjs.net/).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/react-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/react-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/react-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx,.mjs --cache",
    "eslint-fix": "eslint . --ext .js,.jsx,.mjs --cache --fix",
    "install-experimental": "npm install react@experimental react-dom@experimental",
    "prettier": "prettier --list-different '**/*.{json,js,jsx,html,md,mjs,mts,ts,tsx}'",
    "prettier-fix": "prettier --write '**/*.{json,js,jsx,html,md,mjs,mts,ts,tsx}'",
    "start": "node \"./Util - 01 - Custom servers/server.js\" 8040",
    "validate": "npm run eslint && npm run prettier"
  },
  "devDependencies": {
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/systemjs": "6.1.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "4.2.1",
    "prettier": "1.19.1",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@babel/standalone": "7.8.4",
    "@reach/router": "1.3.1",
    "core-js": "3.6.4",
    "create-react-class": "15.6.3",
    "htmltojsx": "0.3.0",
    "mime-types": "2.1.26",
    "plugin-typescript": "8.0.0",
    "prop-types": "15.7.2",
    "react": "0.0.0-experimental-f42431abe",
    "react-cache": "2.0.0-alpha.1",
    "react-dom": "0.0.0-experimental-f42431abe",
    "react-redux": "7.1.3",
    "react-router-6.0.0": "npm:react-router@6.0.0-alpha.1",
    "react-router-dom": "5.1.2",
    "react-router-dom-6.0.0": "npm:react-router-dom@6.0.0-alpha.1",
    "react-spring": "8.0.27",
    "react-transition-group": "4.3.0",
    "redux": "4.0.5",
    "scheduler": "0.18.0",
    "serve": "11.3.0",
    "styled-components": "5.0.1",
    "systemjs-0.21.6": "npm:systemjs@0.21.6",
    "systemjs-plugin-babel": "0.0.25",
    "typescript": "3.7.5"
  }
