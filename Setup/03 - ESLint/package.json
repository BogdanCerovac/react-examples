    "@babel/preset-env": "7.8.6",
    "@babel/preset-react": "7.8.3",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "5.1.1",
    "core-js": "3.6.4",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
