{
  "name": "react-babel-cli",
  "description": "Using React with Babel CLI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/react-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "scripts": {
    "build": "babel site/src --out-dir site/build --source-maps",
    "watch": "babel site/src --out-dir site/build --watch --source-maps"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "core-js": "3.6.5"
  }
}
